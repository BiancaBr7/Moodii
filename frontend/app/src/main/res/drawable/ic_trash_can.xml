<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="24dp"
    android:height="24dp"
    android:viewportWidth="24"
    android:viewportHeight="24">

    <!-- Trash can lid - first rect -->
    <path
        android:fillColor="@color/pixelated_purple_lid"
        android:strokeColor="@color/pixelated_purple_darker"
        android:strokeWidth="2"
        android:strokeLineCap="butt"
        android:strokeLineJoin="miter"
        android:pathData="M5 4H19V6H5Z" /> <!-- Equivalent to <rect x="5" y="4" width="14" height="2"/> -->

    <!-- Trash can lid - second rect -->
    <path
        android:fillColor="@color/pixelated_purple_lid"
        android:strokeColor="@color/pixelated_purple_darker"
        android:strokeWidth="2"
        android:strokeLineCap="butt"
        android:strokeLineJoin="miter"
        android:pathData="M8 6H16V7H8Z" /> <!-- Equivalent to <rect x="8" y="6" width="8" height="1"/> -->

    <!-- Trash can body -->
    <path
        android:fillColor="@color/pixelated_purple_fill"
        android:strokeColor="@color/pixelated_purple_darker"
        android:strokeWidth="2"
        android:strokeLineCap="butt"
        android:strokeLineJoin="miter"
        android:pathData="M6 7 L6 20 L18 20 L18 7 Z" />

    <!-- Lines on the trash can for detail -->
    <path
        android:strokeColor="@color/pixelated_purple_darker"
        android:strokeWidth="2"
        android:strokeLineCap="butt"
        android:strokeLineJoin="miter"
        android:pathData="M10 9 L10 18" /> <!-- Equivalent to <line x1="10" y1="9" x2="10" y2="18"/> -->

    <path
        android:strokeColor="@color/pixelated_purple_darker"
        android:strokeWidth="2"
        android:strokeLineCap="butt"
        android:strokeLineJoin="miter"
        android:pathData="M14 9 L14 18" /> <!-- Equivalent to <line x1="14" y1="9" x2="14" y2="18"/> -->
</vector>